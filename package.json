{
  "name": "cota-parlamentar-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "react": "^16.1.1",
    "react-css-loaders": "0.0.5",
    "react-dom": "^16.1.1",
    "react-scripts": "1.0.17"
  },
  "proxy": "http://localhost:3001",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp -r ./build ../cota-parlamentar",
    "prebuild": "rm -rf build && cd ../cota-parlamentar && rm -rf build",
    "deploy": "cd ../cota-parlamentar && git push heroku deploy:master",
    "predeploy": "cd ../cota-parlamentar && git add . && git commit -m 'latest version'",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}